{
    "version": "0.2.0",
    "configurations": [{
            "type": "node",
            "request": "launch",
            "name": "Run Express Program",
            "program": "${workspaceRoot}/src/express.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Webpack Runner",
            "program": "${workspaceRoot}/webpack-runner.js"
        },
        {
            "name": "Debug Main Process",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd",
            "program": "${workspaceRoot}/window/main.js"
        }
    ],

    "compounds": []
}